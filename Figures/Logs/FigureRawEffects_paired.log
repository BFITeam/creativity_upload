. cap program drop ttest_output

. program define ttest_output
  1.         args file_name  diff_control ub_control lb_control ///
>                                         diff_gift ub_gift lb_gift ///
>                                         diff_turnier ub_turnier lb_turnier
  2.                                         
. 
.         preserve
  3.         clear
  4.         set obs 3
  5.         gen treatment = _n
  6.         gen coefficient = .
  7.         gen low = .
  8.         gen high = .
  9. 
.         local i = 1
 10.         foreach treatment in control gift turnier {
 11.                 replace coefficient = `diff_`treatment'' in `i'
 12.                 replace high = `ub_`treatment'' in `i'
 13.                 replace low = `lb_`treatment'' in `i'
 14.                 local i = `i' + 1
 15.         }
 16.         save "$mypath\Figures\Data\\`file_name'.dta", replace
 17. 
.         restore
 18.         
. end

